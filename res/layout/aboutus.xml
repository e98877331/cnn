				<TextView android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/textView2"  android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/textView1" android:layout_centerHorizontal="true" android:layout_marginTop="10dp" android:text="西岸馬達ＷＣＭ"></TextView>
