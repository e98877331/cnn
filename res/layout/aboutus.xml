                <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/textView8"  android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/textView7" android:layout_centerHorizontal="true" android:layout_marginTop="20dp" android:text="[Server &amp; Client Programming]"></TextView>
